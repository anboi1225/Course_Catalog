<h1>Instructor#show</h1>
<p>Find me in app/views/instructor/show.html.erb</p>

<form name="sign_in_form" action="../user/sign_out" method="get" accept-charset="utf-8">
<div id="topnavigationbar" class="row">
  <div>
    <%= link_to "Home", user_sign_up_path %>
    <%= link_to "Subjects", user_sign_up_path %>
    <%= link_to "Courses", course_show_path %>
    <%= link_to "Instructors", user_sign_up_path %>
    <input type="submit" value="Sign Out" name="sign_out_button">
  </div>
</div>
</form>

<% Instructor.all.each do |instructor| %>
  <%= instructor.first %><br>
  <%= instructor.last %><br>
  <%= instructor.type %><br>
<% end %>
